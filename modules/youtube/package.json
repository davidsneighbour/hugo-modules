{
  "name": "@davidsneighbour/hugo-youtube",
  "description": "A better YouTube shortcode (and partial) for GoHugo",
  "version": "1.2025.3",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/davidsneighbour/hugo-modules",
  "author": "Patrick Kollitsch <patrick@davids-neighbour.com>",
  "bugs": {
    "url": "https://github.com/davidsneighbour/hugo-modules/issues"
  },
  "homepage": "https://github.com/davidsneighbour/hugo-modules/tree/main/modules/youtube",
  "devDependencies": {
    "@davidsneighbour/release-config": "2025.3.8",
    "@playwright/test": "^1.57.0"
  },
  "scripts": {
    "release": "commit-and-tag-version --release-as patch -a -t \"modules/youtube/v\" --releaseCommitMessageFormat \"chore(release): modules/youtube/v{{currentTag}}\" && ./bin/repo/release/postrelease",
    "release:minor": "commit-and-tag-version --release-as minor -a -t \"modules/youtube/v\" --releaseCommitMessageFormat \"chore(release): modules/youtube/v{{currentTag}}\" && ./bin/repo/release/postrelease",
    "test": "playwright test",
    "test:headed": "playwright test --project=chromium --headed"
  },
  "type": "module"
}
